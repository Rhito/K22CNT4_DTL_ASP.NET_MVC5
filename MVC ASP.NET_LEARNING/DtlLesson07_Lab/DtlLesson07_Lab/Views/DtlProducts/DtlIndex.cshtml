@model IEnumerable<DtlLesson07_Lab.Models.DtlProduct>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DtlProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtlPrice)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DtlProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DtlPrice)
        </td>
        <td>
            @Html.ActionLink("Edit", "DtlEdit", new { id=item.DtlProductId }) |
            @Html.ActionLink("Details", "DtlDetails", new { id=item.DtlProductId }) |
            @Html.ActionLink("Delete", "DtlDelete", new { id=item.DtlProductId })
        </td>
    </tr>
}

</table>
