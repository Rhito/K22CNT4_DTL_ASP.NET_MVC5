@model IEnumerable<DtlLesson06.Models.DtlSong>

@{
    ViewBag.Title = "Dtl: Danh sách bài hát";
}

<h2>Dtl: Danh sách bài hát</h2>

<p>
    @Html.ActionLink("Create New", "DtlCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DtlTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtlAuthor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtlArtist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtlYearRelease)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DtlTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DtlAuthor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DtlArtist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DtlYearRelease)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
